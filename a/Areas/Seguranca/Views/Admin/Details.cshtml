@model a.Areas.Seguranca.Models.Usuario
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Listagem de USUÁRIOS";
}
<div class="panel panel-primary">
    <div class="panel-heading">
        Visualizando detalhes de um USUÁRIOS
    </div>
    <div class="panel-body">
        @Html.Partial("~/Area/Seguranca/Views/Usuario/_PartialEditContentPanel.cshtml", Model)
    </div>
    <div class="panel-footer panel-info">
        @Html.ActionLink("Alterar", "Edit", new { id = Model.Id }, new { @class = "btn btn-info" })
        @Html.ActionLink("Retornar para a listagem", "Index", null, new { @class = "btn btn-info" })
    </div>
</div>
@Html.Partial("~/Views/Usuario", Model.Id.ToList())
